Cell[CellGroupData[{

Cell["Summary", "Subsection",
 CellChangeTimes->{{3.5417247740625*^9, 
  3.541724775171875*^9}},ExpressionUUID->"b3c5d5fa-4f9b-8c41-9df3-\
c13f3e17aea9"],

Cell[TextData[{
 "To solve ",
 Cell[BoxData[
  FormBox[
   RowBox[{"G", "=", 
    RowBox[{"\[LessLess]", 
     RowBox[{
      SubscriptBox[
       OverscriptBox["a", "^"], "\[Sigma]"], 
      SubsuperscriptBox[
       OverscriptBox["a", "^"], "\[Sigma]", "+"]}], "\[GreaterGreater]"}]}], 
   TraditionalForm]],ExpressionUUID->"3165f44b-e9dd-4644-8bf8-0afb70a83946"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"H", "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["\[Epsilon]", "a"], 
      RowBox[{
       SubscriptBox["\[Sum]", "\[Sigma]"], 
       RowBox[{
        SubsuperscriptBox[
         OverscriptBox["a", "^"], "\[Sigma]", "+"], 
        SubscriptBox[
         OverscriptBox["a", "^"], "\[Sigma]"]}]}]}], "+", 
     RowBox[{
      UnderscriptBox["\[Sum]", "\[Alpha]\[Sigma]"], 
      RowBox[{
       SubscriptBox["\[Epsilon]", "\[Alpha]\[Sigma]k"], 
       SubsuperscriptBox[
        OverscriptBox["c", "^"], "\[Alpha]\[Sigma]k", "+"], 
       SubscriptBox[
        OverscriptBox["c", "^"], "\[Alpha]\[Sigma]k"]}]}], "+", 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        UnderscriptBox["\[Sum]", "\[Alpha]\[Sigma]k"], 
        RowBox[{
         SubscriptBox["t", "\[Alpha]k"], 
         SubsuperscriptBox[
          OverscriptBox["c", "^"], "\[Alpha]\[Sigma]k", "+"], 
         SubscriptBox[
          OverscriptBox["a", "^"], "\[Sigma]"]}]}], "+", 
       RowBox[{"h", ".", "c", "."}]}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"8aa097f0-09c3-8b44-a120-f86e024b768a"],
 ", you need to have the following inputs:"
}], "Text",
 CellChangeTimes->{{3.5446130786875*^9, 3.544613083765625*^9}, {
  3.54461314953125*^9, 
  3.54461317790625*^9}},ExpressionUUID->"23cf9373-5311-1248-8b9e-\
d6a164db50b2"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"H", "=", 
    RowBox[{"Hlr", "+", "Htld", "+", "Htrd", "+", "Hdot"}]}], ";"}], 
  "   "}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Hlr", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Epsilon]l1", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cl1dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox[
       RowBox[{"cl1", "[", "k", "]"}], "^"]}]}], "+", 
    RowBox[{
     RowBox[{"\[Epsilon]l2", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cl2dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox[
       RowBox[{"cl2", "[", "k", "]"}], "^"]}]}], "+", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Epsilon]r1", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cr1dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox[
       RowBox[{"cr1", "[", "k", "]"}], "^"]}]}], "+", 
    RowBox[{
     RowBox[{"\[Epsilon]r2", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cr2dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox[
       RowBox[{"cr2", "[", "k", "]"}], "^"]}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Hdot", "=", 
   RowBox[{
    RowBox[{"\[Epsilon]a", " ", 
     RowBox[{
      OverscriptBox["a1dag", "^"], "**", 
      OverscriptBox["a1", "^"]}]}], "+", 
    RowBox[{"\[Epsilon]a", " ", 
     RowBox[{
      OverscriptBox["a2dag", "^"], "**", 
      OverscriptBox["a2", "^"]}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Htld", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"tl", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cl1dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox["a1", "^"]}]}], "+", 
    RowBox[{
     RowBox[{"tlstar", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox["a1dag", "^"], "**", 
      OverscriptBox[
       RowBox[{"cl1", "[", "k", "]"}], "^"]}]}], "+", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"tl", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cl2dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox["a2", "^"]}]}], "+", 
    RowBox[{
     RowBox[{"tlstar", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox["a2dag", "^"], "**", 
      OverscriptBox[
       RowBox[{"cl2", "[", "k", "]"}], "^"]}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Htrd", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"tr", "[", "k", "]"}], " ", 
     RowBox[{
      OverscriptBox[
       RowBox[{"cr1dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox["a1", "^"]}]}], "+", 
    RowBox[{
     RowBox[{"tr", "[", "k", "]"}], " ", 
     RowBox[{
      OverscriptBox[
       RowBox[{"cr2dag", "[", "kr2", "]"}], "^"], "**", 
      OverscriptBox["a2", "^"]}]}], "+", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"trstar", "[", "k", "]"}], " ", 
     RowBox[{
      OverscriptBox["a1dag", "^"], "**", 
      OverscriptBox[
       RowBox[{"cr1", "[", "k", "]"}], "^"]}]}], "+", 
    RowBox[{
     RowBox[{"trstar", "[", "k", "]"}], " ", 
     RowBox[{
      OverscriptBox["a2dag", "^"], "**", 
      OverscriptBox[
       RowBox[{"cr2", "[", "k", "]"}], "^"]}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetRules", "[", " ", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"cl1", "[", "k_", "]"}], ",", 
       RowBox[{"cl1dag", "[", "kp_", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"cl1", "[", "k_", "]"}], 
         RowBox[{"cl1dag", "[", "kp_", "]"}]}], "\[RuleDelayed]", " ", 
        RowBox[{"DiracDelta", "[", 
         RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"cr1", "[", "k_", "]"}], ",", 
       RowBox[{"cr1dag", "[", "kp_", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"cr1", "[", "k_", "]"}], 
         RowBox[{"cr1dag", "[", "kp_", "]"}]}], "\[RuleDelayed]", " ", 
        RowBox[{"DiracDelta", "[", 
         RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"cl2", "[", "k_", "]"}], ",", 
       RowBox[{"cl2dag", "[", "kp_", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"cl2", "[", "k_", "]"}], 
         RowBox[{"cl2dag", "[", "kp_", "]"}]}], "\[RuleDelayed]", " ", 
        RowBox[{"DiracDelta", "[", 
         RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"cr2", "[", "k_", "]"}], ",", 
       RowBox[{"cr2dag", "[", "kp_", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"cr2", "[", "k_", "]"}], 
         RowBox[{"cr2dag", "[", "kp_", "]"}]}], "\[RuleDelayed]", " ", 
        RowBox[{"DiracDelta", "[", 
         RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"a1", ",", "a1dag", ",", "1", ",", "1"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"a2", ",", "a2dag", ",", "1", ",", "1"}], "}"}]}], 
    "\[IndentingNewLine]", "}"}], " ", "]"}], " "}], "\[IndentingNewLine]", 
 RowBox[{"SetSumSub", "[", 
  RowBox[{"{", "k", "}"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"SetPreserve", "[", 
  RowBox[{"{", 
   RowBox[{
    OverscriptBox["a1", "^"], ",", 
    OverscriptBox["a1dag", "^"], ",", 
    OverscriptBox["a2", "^"], ",", 
    OverscriptBox["a2dag", "^"]}], "}"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"TargetGF", "[", 
  RowBox[{
   OverscriptBox["a1", "^"], "**", 
   OverscriptBox["a1dag", "^"]}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"DeriveGF", "[", "H", "]"}]}], "Input",
 CellChangeTimes->{{3.5446132344375*^9, 
  3.5446132801875*^9}},ExpressionUUID->"f372f440-5c47-2c4e-80db-50581ab29eed"],

