definitions: ",
 Cell[BoxData[
  RowBox[{
   OverscriptBox[
    RowBox[{"\[CapitalSigma]1", "[", 
     OverscriptBox["j1", "\[RightVector]"], "]"}], "~"], "=", 
   RowBox[{
    SubscriptBox["\[Sum]", "j1"], 
    RowBox[{
     RowBox[{"tl", "[", "j1", "]"}], " ", 
     RowBox[{"tlstar", "[", "j1", "]"}], " ", 
     UnderscriptBox[
      RowBox[{"gi2", "[", "j1", "]"}], "_"]}]}]}]],
  CellChangeTimes->{3.54172399521875*^9},ExpressionUUID->
  "ddfa948c-493c-f74c-9e48-0171bcab3a03"],
 "; and similarly for ",
 Cell[BoxData[
  OverscriptBox[
   RowBox[{"\[CapitalSigma]2", "[", 
    OverscriptBox["j2", "\[RightVector]"], "]"}], "~"]],
  CellChangeTimes->{3.541724087625*^9},ExpressionUUID->
  "9341cff8-5113-4c45-a8ec-7f784d08c314"],
 "."
}], "Text",
