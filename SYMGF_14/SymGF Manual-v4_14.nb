      RowBox[{"GF1", "[", "]"}], "_"], " ", 
     OverscriptBox[
      RowBox[{"\[CapitalSigma]2", "[", 
       OverscriptBox["j2", "\[RightVector]"], "]"}], "~"], " ", 
     UnderscriptBox[
      RowBox[{"gi1", "[", "]"}], "_"]}]}]}]],
  CellChangeTimes->{3.54172399521875*^9},ExpressionUUID->
  "f0b79950-acc3-314e-a4ef-ab14b038cde9"],
