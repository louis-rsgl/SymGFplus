Cell[CellGroupData[{

Cell["Supplying the anti-commutation rules", "Subsection",
 CellChangeTimes->{{3.541636002609375*^9, 3.5416360110625*^9}, {
  3.5426373720519032`*^9, 3.542637373097105*^9}, {3.543887676421875*^9, 
  3.543887676953125*^9}},ExpressionUUID->"68898c62-59af-6247-a9d3-\
eacd8cd71a8f"],

Cell[TextData[{
 "At this point, ",
 StyleBox["SymGF",
  FontSlant->"Italic"],
 " doesn't know how operators we used in the Hamiltonian anti-commute. \
Suppose the operator ",
 Cell[BoxData[
  FormBox[
   OverscriptBox[
    SubscriptBox["a", "k"], "^"], TraditionalForm]],ExpressionUUID->
  "fd335f03-09a2-4e41-b0e6-119e513153b4"],
 " has the property ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      OverscriptBox[
       SubscriptBox["a", "k"], "^"], ",", 
      SubsuperscriptBox[
       OverscriptBox["a", "^"], 
       RowBox[{"k", "'"}], "+"]}], "}"}], "=", 
    SubscriptBox["\[Delta]", 
     RowBox[{"k", " ", 
      RowBox[{"k", "'"}]}]]}], TraditionalForm]],ExpressionUUID->
  "6cddece1-3d2e-4a48-a81d-ad3cea8b4f4d"],
 ", then we should type the following ",
 StyleBox["SetRules[]",
  FontFamily->"Courier New",
  FontWeight->"Bold"],
 " statement into ",
 StyleBox["SymGF",
  FontSlant->"Italic"],
 ":"
}], "Text",
 CellChangeTimes->{{3.541636024578125*^9, 3.5416362718125*^9}, {
  3.54411973196875*^9, 
  3.54411973203125*^9}},ExpressionUUID->"69da7293-4a32-b14d-abca-\
f1fb7ca777ce"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"SetRules", "[", 
  RowBox[{"{", "  ", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"a", "[", "k_", "]"}], ",", 
     RowBox[{"adag", "[", "kp_", "]"}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"a", "[", "k_", "]"}], 
       RowBox[{"adag", "[", "kp_", "]"}]}], "\[RuleDelayed]", 
      RowBox[{"DiracDelta", "[", 
       RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], "  ", "}"}], 
  "]"}]], "Input",
 CellChangeTimes->{{3.541636288828125*^9, 3.54163633065625*^9}, 
   3.541643184734375*^9, {3.543971859875*^9, 3.543971860484375*^9}, {
   3.54397196171875*^9, 
   3.543971961984375*^9}},ExpressionUUID->"6290112d-ae39-fa41-a6e5-\
376726f7eeaf"],

Cell[CellGroupData[{

Cell[BoxData["\<\"(Anti-)Commutation relation(s): \"\>"], "Print",
 CellChangeTimes->{{3.543971837515625*^9, 3.54397186121875*^9}, 
   3.54397189534375*^9, {3.54397196284375*^9, 3.543971980921875*^9}, 
   3.54461439753125*^9},ExpressionUUID->"157fde24-4d87-8045-a4b2-\
799ce94f30e8"],

Cell[BoxData[
 TagBox[GridBox[{
    {"1", "\<\":  {\"\>", 
     RowBox[{"a", "[", "k_", "]"}], "\<\",\"\>", 
     RowBox[{"adag", "[", "kp_", "]"}], "\<\"} =\"\>", 
     RowBox[{"DiracDelta", "[", 
      RowBox[{"k", "-", "kp"}], "]"}]}
   },
   GridBoxAlignment->{
    "Columns" -> {{Left}}, "ColumnsIndexed" -> {}, "Rows" -> {{Baseline}}, 
     "RowsIndexed" -> {}},
   GridBoxSpacings->{"Columns" -> {
       Offset[0.27999999999999997`], {
        Offset[0.7]}, 
