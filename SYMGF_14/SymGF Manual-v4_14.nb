Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"H", "=", 
    RowBox[{"Hlr", "+", "Htld", "+", "Htrd", "+", "Hdot"}]}], ";"}], "   ", 
  RowBox[{"(*", " ", 
   RowBox[{
   "Total", " ", "Hamiltonian", " ", "is", " ", "composed", " ", "of", " ", 
    "4", " ", "parts"}], " ", "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Hlr", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"\[Epsilon]l1", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cl1dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox[
       RowBox[{"cl1", "[", "k", "]"}], "^"]}]}], "+", 
    RowBox[{
     RowBox[{"\[Epsilon]l2", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cl2dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox[
       RowBox[{"cl2", "[", "k", "]"}], "^"]}]}], "+", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Epsilon]r1", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cr1dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox[
       RowBox[{"cr1", "[", "k", "]"}], "^"]}]}], "+", 
    RowBox[{
     RowBox[{"\[Epsilon]r2", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cr2dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox[
       RowBox[{"cr2", "[", "k", "]"}], "^"]}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Hdot", "=", 
   RowBox[{
    RowBox[{"\[Epsilon]a", " ", 
     RowBox[{
      OverscriptBox["a1dag", "^"], "**", 
      OverscriptBox["a1", "^"]}]}], "+", 
    RowBox[{"\[Epsilon]a", " ", 
     RowBox[{
      OverscriptBox["a2dag", "^"], "**", 
      OverscriptBox["a2", "^"]}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Htld", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"tl", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cl1dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox["a1", "^"]}]}], "+", 
    RowBox[{
     RowBox[{"tlstar", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox["a1dag", "^"], "**", 
      OverscriptBox[
       RowBox[{"cl1", "[", "k", "]"}], "^"]}]}], "+", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"tl", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox[
       RowBox[{"cl2dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox["a2", "^"]}]}], "+", 
    RowBox[{
     RowBox[{"tlstar", "[", "k", "]"}], 
     RowBox[{
      OverscriptBox["a2dag", "^"], "**", 
      OverscriptBox[
       RowBox[{"cl2", "[", "k", "]"}], "^"]}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Htrd", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"tr", "[", "k", "]"}], " ", 
     RowBox[{
      OverscriptBox[
       RowBox[{"cr1dag", "[", "k", "]"}], "^"], "**", 
      OverscriptBox["a1", "^"]}]}], "+", 
    RowBox[{
     RowBox[{"tr", "[", "k", "]"}], " ", 
     RowBox[{
      OverscriptBox[
       RowBox[{"cr2dag", "[", "kr2", "]"}], "^"], "**", 
      OverscriptBox["a2", "^"]}]}], "+", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"trstar", "[", "k", "]"}], " ", 
     RowBox[{
      OverscriptBox["a1dag", "^"], "**", 
      OverscriptBox[
       RowBox[{"cr1", "[", "k", "]"}], "^"]}]}], "+", 
    RowBox[{
     RowBox[{"trstar", "[", "k", "]"}], " ", 
     RowBox[{
      OverscriptBox["a2dag", "^"], "**", 
      OverscriptBox[
       RowBox[{"cr2", "[", "k", "]"}], "^"]}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetRules", "[", " ", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"cl1", "[", "k_", "]"}], ",", 
       RowBox[{"cl1dag", "[", "kp_", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"cl1", "[", "k_", "]"}], 
         RowBox[{"cl1dag", "[", "kp_", "]"}]}], "\[RuleDelayed]", " ", 
        RowBox[{"DiracDelta", "[", 
         RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"cr1", "[", "k_", "]"}], ",", 
       RowBox[{"cr1dag", "[", "kp_", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"cr1", "[", "k_", "]"}], 
         RowBox[{"cr1dag", "[", "kp_", "]"}]}], "\[RuleDelayed]", " ", 
        RowBox[{"DiracDelta", "[", 
         RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"cl2", "[", "k_", "]"}], ",", 
       RowBox[{"cl2dag", "[", "kp_", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"cl2", "[", "k_", "]"}], 
         RowBox[{"cl2dag", "[", "kp_", "]"}]}], "\[RuleDelayed]", " ", 
        RowBox[{"DiracDelta", "[", 
         RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"cr2", "[", "k_", "]"}], ",", 
       RowBox[{"cr2dag", "[", "kp_", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"cr2", "[", "k_", "]"}], 
         RowBox[{"cr2dag", "[", "kp_", "]"}]}], "\[RuleDelayed]", " ", 
        RowBox[{"DiracDelta", "[", 
         RowBox[{"k", "-", "kp"}], "]"}]}], ",", "1"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"a1", ",", "a1dag", ",", "1", ",", "1"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"a2", ",", "a2dag", ",", "1", ",", "1"}], "}"}]}], 
    "\[IndentingNewLine]", "}"}], " ", "]"}], " "}], "\[IndentingNewLine]", 
 RowBox[{"SetSumSub", "[", 
  RowBox[{"{", "k", "}"}], "]"}]}], "Input",
 CellChangeTimes->{{3.541720345140625*^9, 3.54172035646875*^9}, {
   3.541720396515625*^9, 3.541720426390625*^9}, {3.541720494703125*^9, 
