   RowBox[{"H", "=", 
    RowBox[{
     SubscriptBox["\[Sum]", "k"], 
     RowBox[{
      SubscriptBox["\[Epsilon]", "k"], 
      SubsuperscriptBox[
       OverscriptBox["a", "^"], "k", "+"], 
      SubscriptBox[
       OverscriptBox["a", "^"], "k"]}]}]}], TraditionalForm]], "None",
